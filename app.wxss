/**app.wxss**/
.container {
  width:100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
  
} 
page{
  width: 100%;
  height:100%;
 
}
/*提示信息（未获取到数据时，服务器返回的提示语）  */
cover-view{
  width:350rpx;
  height:90rpx;
  background-color: #bbb;
  position: absolute;
  top:50%;
  margin-top: -45rpx;
  left:200rpx;
  border-radius: 15rpx;
  font-size: 30rpx;
  text-align: center;
  line-height: 90rpx;
  color: #fff;
  display: none;
}
