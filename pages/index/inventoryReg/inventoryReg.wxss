/* pages/index/inventoryReg/inventoryReg.wxss */
@import "../../../lib/style/font.wxss";
.iconfont {
  font-family:"iconfont" !important;
  font-size:37rpx;
  color: #E7612E;
}
.inventoryReg{
  width: 100%;
 
  
}
.inventoryReg>.head{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1px solid #E7612E;
 
}
.inventoryReg>.head>i{
  float: right;
  height: 37rpx;
  margin-right: 30rpx;
}
.inventoryList{
  width: 100%;
  padding: 0 25rpx;
  box-sizing: border-box;
  font-size: 23rpx;
  height: 215rpx;
  border-bottom: 20rpx solid #EFEFF2;
}
.inventoryList view{
  margin-top: 20rpx;
}
.inventoryList text{
  color: red;
}
.noneData{
  width: 100%;
  color: #B9B9B9;
  text-align: center;
  padding-top: 430rpx;
 
}
.noneData .icon-kulian{
  color: #B9B9B9;
  font-size: 120rpx;
}
.noneData view{
  font-size: 25rpx;
}