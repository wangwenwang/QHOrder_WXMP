/* pages/index/manageInventory/warehouseDetail/warehouseDetail.wxss */
.warehouseDetail{
  width: 100%;
  font-size: 26rpx;
}
.warehouseDetail>.head{
  width: 100%;
  height: 60rpx;
  line-height: 60rpx;
  border-bottom: 1px solid #E7612E;
 
}
.warehouseDetail>.head>text{
  float: right;
  height: 37rpx;
  margin-right: 30rpx;
  color: #E7612E;
}
.warehouseDetail text{
  color: red;
}
.goodsInfo{
  width: 100%;
  background-color: #E7E7EC;
  padding: 23rpx 27rpx;
  box-sizing: border-box;
   margin: 30rpx 0;
}
.goodsInfo>view{
  margin-bottom: 10rpx;
 
}
.warehouseList{
  width:100%;
  height: 140rpx;
  background-color: #E7E7EC;
  margin-bottom: 10rpx;
  padding: 15rpx 27rpx;
  box-sizing: border-box;
}
.warehouseList>view{
  margin-bottom: 5rpx;
}
.date{
  overflow: hidden;
}
.date>view:first-child{
  float: left;
}
.date>view:last-child{
  float: right;
}