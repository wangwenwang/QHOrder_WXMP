<!--pages/search/orderDetail/goodsTemplate/goodsTemplate.wxml-->
<template name="goodsTemplate">
 
    <icon class="iconfont icon-liwu"></icon>
    <view class='listRight'>
      <view>订单编号：
        <text>{{PRODUCT_NO}}</text>
      </view>
      <view>货物名称：
        <text>{{PRODUCT_NAME}}</text>
      </view>
      <view class='bottom'><view>数量：<text>{{ISSUE_QTY}}箱</text></view><view>原价：<text>¥{{ORG_PRICE}}</text></view></view>
      <view class='bottom'><view>重量：<text>{{ORDER_WEIGHT}}吨</text></view><view>付款价：<text>¥{{ACT_PRICE}}</text></view></view>
      <view class='bottom'><view>体积：<text>{{ORDER_VOLUME}}m³</text></view><view>总价：<text>¥{{totalPrice}}</text></view></view>
    </view>
 

</template>
