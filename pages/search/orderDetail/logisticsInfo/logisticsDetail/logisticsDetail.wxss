/* pages/search/orderDetail/logisticsInfo/logisticsDetail/logisticsDetail.wxss */
.logisticsDetail{
  width:100%;
  height:100%;
  background-color: #EDEDF2;
  font-size: 23rpx;
}
.logisticsDetail>.orderInfo{
  
  padding:10rpx 16rpx;
  box-sizing: border-box;
  overflow: hidden;
 
}
.logisticsDetail>.orderInfo>view{
  margin: 10rpx;
  overflow: hidden;
}
.logisticsDetail>.orderInfo>view.infoHead,.goodsHead{
  font-size: 29rpx;
  margin: 0 10rpx 25rpx;
  width: 300rpx;
}
.logisticsDetail>.orderInfo>view text{
  color: red;
}
.logisticsDetail>.orderInfo>view{
  overflow: hidden;
}
.logisticsDetail>.orderInfo>view>view{
  float: left;
  width: 347rpx;
}
.logisticsDetail>.goodsInfo{
  padding: 0 8rpx;
  box-sizing: border-box;
  padding-bottom: 12rpx;
  background-color: #EDEDF2;
 
}
.goodsList{
  width:100%;
  height:180rpx;
  overflow: hidden;
  background-color: #fff;
  border-radius: 10rpx;
  margin-bottom: 5rpx;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.goodsList>view{
  height: 60rpx;
  line-height: 60rpx;
}
.goodsList text{
  color: red;
}
.bottom{
  overflow: hidden;
}
.bottom>view{
  float: left;
  width:337rpx;
 
}

