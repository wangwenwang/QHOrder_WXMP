/* pages/search/search.wxss */
@import "searchTemplate/searchTemplate.wxss";
@import "../../lib/style/font.wxss";
.iconfont {
  font-family:"iconfont" !important;
  font-size:128rpx;
  margin-bottom: 25rpx;


}
.searchContainer{
  width: 100%;
  
}
.searchContainer>.searchHead{
  width:100%;
  height:80rpx;
  background-color: #F2F2F2;
  display: flex;
  position: fixed;
  top:0;
  left:0;
}
.searchContainer>.searchHead>view{
  flex-grow: 1;
  text-align: center;
  height:80rpx;
  line-height: 80rpx;
  font-size: 28rpx;
  font-weight: 600;
  box-sizing: border-box;
}
.searchActive{
  color: #A71E23;
  border-bottom: 1px solid #A71E23;
}

.searchResult{
  width: 100%;
  padding-top: 80rpx;
}
.searchTotal{
  color: #3C3C3C;
  font-size: 22rpx;
  text-align: center;
  font-weight: 600;
  width: 100%;
  height:80rpx;
  line-height: 80rpx;
}

.nullNote{
  width: 100%;
  height:226rpx;
  color: #D5D5D5;
  text-align: center;
  position: absolute;
  top:400rpx;
  left: 0;
}